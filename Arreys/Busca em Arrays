        Busca Binaria==

incio = 0;
fim = V.length - 1;
do {
  pivo = (inicio + fim) /2;
  if( chave > V[pivo]){     //chave maior que
    inicio = pivo +1;
  } else {
    if( chave < V[pivo] ) {
      fim = pivo -1;
    }
  }
} while (chave != V[pivo] &&
          inicio <= fim );
          // fim > inicio
if( inicio > fim ) {
    //nao encontrou
} else {
    //encontrou
    // posicao = pivo
}



